{
  "name": "cc-quota",
  "version": "0.1.2",
  "description": "Fetch Claude Code OAuth usage statistics (5-hour and 7-day limits)",
  "type": "module",
  "bin": {
    "cc-quota": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lib": {
      "types": "./dist/usage.d.ts",
      "default": "./dist/usage.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsdown",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepack": "pnpm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "usage",
    "quota",
    "oauth",
    "api",
    "cli",
    "limits"
  ],
  "author": "eyle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyle-i/cc-quota.git"
  },
  "bugs": {
    "url": "https://github.com/eyle-i/cc-quota/issues"
  },
  "homepage": "https://github.com/eyle-i/cc-quota#readme",
  "dependencies": {
    "gunshi": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^25.2.0",
    "clean-pkg-json": "^1.2.0",
    "eslint": "^9.18.0",
    "tsdown": "^0.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=18"
  }
}
